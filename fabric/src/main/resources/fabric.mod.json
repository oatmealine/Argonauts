{
    "schemaVersion": 1,
    "id": "argonauts",
    "version": "${version}",
    "name": "Argonauts",
    "description": "A guild and party mod to work and play together with your teammates on a server!",
    "authors": [
        "Alex Nijjar",
        "ThatGravyBoat"
    ],
    "contributors": [
    ],
    "contact": {
        "website": "https://modrinth.com/mod/argonauts",
        "repo": "https://github.com/terrarium-earth/argonauts",
        "issues": "https://github.com/terrarium-earth/argonauts/issues"
    },
    "license": "MIT",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "earth.terrarium.argonauts.fabric.ArgonautsFabric"
        ],
        "client": [
            "earth.terrarium.argonauts.client.fabric.ArgonautsClientFabric"
        ],
        "rei_client": [
            "earth.terrarium.argonauts.client.compat.rei.ArgonautsReiClientPlugin"
        ]
    },
    "mixins": [
        "argonauts-common.mixins.json"
    ],
    "depends": {
        "fabric": "*",
        "minecraft": ">=1.20",
        "resourcefullib": ">=2.1.1"
    }
}
